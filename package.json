{
  "name": "lucknow-hotel",
  "version": "1.0.0",
  "description": "Website for a fictitious hotel in San Francisco, CA",
  "main": "index.js",
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "Alex Ha",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^4.1.2",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "mongoose": "^5.6.11",
    "nodemailer": "^6.3.0",
    "stripe": "^7.8.0"
  }
}
